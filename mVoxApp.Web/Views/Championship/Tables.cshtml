@model IEnumerable<mVoxApp.Web.Models.TeamModel>

@{
    ViewBag.Title = "Tables";
    Layout = "~/Views/Shared/_Layout.cshtml";
    if ( @ViewBag.alert2 != null )
    {        
        <script>
            var texto = "@ViewBag.alert";
            alert(texto);
        </script>
    }
}

<br /><br />

<h2 class="text-center danger">@ViewBag.KeyGroupFull</h2>

<hr /><br /><br /><br />
<div>
    <h2 class="text-center">Tabela das Etapas</h2>
    @Html.Partial("PartialView/_TablePView_keygroup", ViewData["keygroups"])
</div>

<hr />
<div>
    @{
        int count = 1;
        foreach (var item in ViewData["keygroups-all"] as IEnumerable<mVoxApp.Web.Models.KeyGroupModel>)
        {
            count++;
        }

        <h5 class="text-center">Grupos de Classificação: @(count - 1)</h5>

        if (ViewData["teams-all"] != null)
        {
            if (ViewData.Values != null && ViewData.Values.Count() > 0)
            {
                for (int i = 1; i < count; i++)
                {
                    <hr /><br />
                    <hw>
                        <h2 class="text-center">
                            Group @(i): @ViewData[$"keygroupname{i}"]
                        </h2>
                    </hw>

                    @Html.Partial("PartialView/_TablePView", ViewData[$"TeamsByKeyGroup{i}"])

                }
            }
        }
    }
</div>




